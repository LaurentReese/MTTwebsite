{
    "project": "mtt-habitat",
    "services": [
      {
        "id": "frontend",
        "apps": [
          {
            "id": "vuejs",
            "image": "32681733/mtt-frontend",
            "instances": 1,
            "mem": 512,
            "domain": {
              "uri": "mtt-habitat.sloppy.zone"
            },
            "ssl": true,
            "port_mappings": [
              {
                "container_port": 8080
              }
            ],
            "volumes": [
            ],
            "health_checks": [
            ],
            "logging": null
          }
        ]
      },
      {
        "id": "backend",
        "apps": [
          {
            "id": "golang",
            "image": "32681733/mtt-backend",
            "instances": 1,
            "mem": 1536,
            "domain": {
              "uri": "mtt-backend.sloppy.zone"
            },
            "ssl": false,
            "port_mappings": [
              {
                "container_port": 80
              }
            ],
            "env": {
              "MTT_EXECUTION": "PRODUCTION"
            },
            "volumes": [
              {
                "container_path": "/data/db",
                "size": "8GB"
              }
            ],
            "health_checks": [
            ],
            "logging": null
          }
        ]
      }
    ]
  }
